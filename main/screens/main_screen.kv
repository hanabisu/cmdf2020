<MainScreen>:
    FloatLayout:
        Label:
            text: "Test"
        GridLayout:
            rows:2
            row_force_default: True
            row_default_height: 50
            pos_hint: {'center_x':0.5}
            size_hint: (None, None)
            Button:
                size: 75, 50
                size_hint: None, None
                text: 'Start App'
                on_press: root.manager.current = 'enter_item'
            Button:
                size: 75, 50
                size_hint: None, None
                text: 'Quit'

